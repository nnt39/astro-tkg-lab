---
import { getLives } from "@/lib/microcms.ts";
const response = await getLives({ fields: ["id", "title", "artist", "venue"] });
---

{
  response.contents.map((content: any) => (
    <div>
      <>
        <p>{content.title}</p>
        <p>{content.artist}</p>
        <p>{content.venue}</p>
      </>
    </div>
  ))
}